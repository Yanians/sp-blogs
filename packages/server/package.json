{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean:both": "rimraf ./dist/babel && rimraf ./dist/webpack",
    "clean:babel": "rimraf ./dist/babel",
    "clean:webpack": "rimraf ./dist/webpack",
    "type-check": "cross-env NODE_ENV=development tsc --noEmit --watch",
    "babel-server": "cross-env NODE_ENV=development babel ./src --out-dir ./dist/babel --extensions '.ts,.tsx' --copy-files --watch",
    "build:type-check": "type-check && cross-env NODE_ENV=development  webpack ./webpack.config.js --watch",
    "build": "cross-env NODE_ENV=development npx webpack  --config  ./webpack.config.js --progress --watch",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/server.tsx",
    "start": "cross-env NODE_ENV=development node ./dist/babel/server.js --watch",
    "watch": "cross-env NODE_ENV=development nodemon ./src/server.tsx"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.27.0",
    "@emotion/server": "^11.11.0",
    "@types/lodash": "^4.17.16",
    "@types/mark.js": "^8.11.12",
    "@types/react-router-dom": "^5.3.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "fs-extra": "^11.3.0",
    "install": "^0.13.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.13.1",
    "npm": "^11.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "uuid": "^11.1.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "compression-webpack-plugin": "^11.1.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
