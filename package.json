  "scripts": {
    "clean:both": "npm run clean:babel -w packages/server && npm run clean:webpack -w packages/server",
    "clean:babel": "npm run clean:babel -w packages/server",
    "clean:server": "npm run clean:webpack -w packages/server",
    "babel-type:client": "concurrently \"npm run type-check -w packages/client \" \" npm run babel-client -w packages/client\"",
    "babel-type:server": "concurrently \"npm run type-check -w packages/server \" \" npm run babel-server -w packages/server\"",
    "build-client:dev": "npm run build:dev -w packages/client",
    "build-client:prod": "npm run build:prod -w packages/client",
    "build-client:serve": "npm run serve -w packages/client",
    "build-server:webpack": "npm run build -w packages/server",
    "dev": "npm run dev -w packages/server",
    "tsnode": "npm run tsnode-dev -w packages/server",
    "start": "npm run start -w packages/server",
    "watch": "npm run watch -w package/server && npm run watch -w packages/client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
