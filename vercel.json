{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/server/src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server/server/src/server.js"
    }
  ],

"env":{
"PUBLIC_URL":"dist/server/server/public/static/images/",
"DATABASE_NAME":"sp-blogs",
"BUILD_PATH":"build",
"NODE_ENV":"production"
  }
}
