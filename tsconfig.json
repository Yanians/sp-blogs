{
  "compilerOptions": {
    "target": "es5",
    "module": "CommonJS",
    "jsx": "react-jsx",
    "moduleResolution":"node",
    "baseUrl": "./", //firmed
    "forceConsistentCasingInFileNames": true,
    "strict": true, //firmed
    "noEmit": true,
    "allowJs":true,
    "declaration": false, // putting this value true will conflict to noEmit:true
    "paths": {
      "@treasury/*": ["./src/mui-treasury/layout-core-v5/*"],
      "@treasury-center/*": ["src/mui-treasury/*"],
      "@lib/*": ["packages/client/src/components/lib/*"],
      "@routes/*": ["packages/client/src/routes/*"],
      "src/*": ["packages/client/src/*"],
      "@reduxsrc/*": ["packages/client/src/redux/reducers/*"],
      "@client/*": ["packages/client/src/*"],
      "@toggleComponent": ["packages/client/src/components/toggleComponent/*"],
      "@imagedir/*": ["packages/client/src/public/static/images/*"],
      "@extractorfile/*": ["packages/extractorfile/*"],
      "@loader/marking/*": ["packages/extractorfile/loader"],
      "@components/*": ["packages/client/src/components/*"]
    },
    "importHelpers": true,
     "allowSyntheticDefaultImports": true,
     "noFallthroughCasesInSwitch": true,
    //  "noUnusedLocals": true,
    //  "noUnusedParameters": true,
    //  "noImplicitReturns": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,

  },
  "include": [],
  "references": [
  { "path": "packages/client" },
  { "path": "packages/server" },
  { "path": "packages/extractorfile" }
],
   "exclude":[
    "node_modules",
    "src/mui-treasury",
    "packages/client/dist",
    "packages/server/dist",
    "packages/server/getUrlOrPath.js",
    "packages/client/webpack.build.js",
    "packages/client/webpack.config.js",
    "packages/server/webpack.config.js"
   ]
}